prototype(NeosRulez.Blog:BlogTitle) {
    templatePath = 'resource://NeosRulez.Blog/Private/Templates/FusionObjects/BlogTitle.html'
    blogTitle = ${q(documentNode).property('blogtitle')}
    @cache {
        mode = 'uncached'
        context {
            1 = 'node'
            2 = 'documentNode'
        }
    }
}