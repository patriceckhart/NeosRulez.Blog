prototype(NeosRulez.Blog:BlogSubTitle) {
    templatePath = 'resource://NeosRulez.Blog/Private/Templates/FusionObjects/BlogSubTitle.html'
    blogSubTitle = ${q(documentNode).property('blogsubtitle')}
    @cache {
        mode = 'uncached'
        context {
            1 = 'node'
            2 = 'documentNode'
        }
    }
}